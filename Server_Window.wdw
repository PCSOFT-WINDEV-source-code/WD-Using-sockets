     name : RefreshTable
     internal_properties : CAAAAAgAAAA7f8CexCaT3KNokcNyRz3oERINaVFAff7BWzNur7RAGMaOzjoTtPiWvcmZOarOmabS49bpawLrSuHukM3D+nCltiHbZ5cw4gVNaoaw4T6GLs2my7QKCZ/uSPs1VtTEg7NEPRfCbBcADPbaifSmiAzSnsKgiai/MvkTyZdSGEwx9M82Ng==
     procedure_id : 1760358308382253750
     type_code : 14
     code : |1-
      PROCEDURE RefreshTable()
      
      nSubscript is int
      sSocketName is string
      
      
      CriticalSectionStart("Management")
      
      // Browse all the connections of the table
      FOR ALL ROW of Table1
      	// Retrieve the name of the socket
      	sSocketName = Socket
      	
      	// The socket is deleted?
      	IF SocketExist(sSocketName)=False THEN
      		TableDelete(Table1)
      		nSubscript--
      	END
      END
      
      
      CriticalSectionEnd("Management")
