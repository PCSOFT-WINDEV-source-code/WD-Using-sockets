     procedure_id : 1760358308382319286
     type_code : 14
     code : |1-
      PROCEDURE StopServer()
      
      // Stop the refresh timer
      EndAutomatedProcedure(RefreshTable)
      
      // Stop the listening thread
      EndAutomatedProcedure(ListenConnections)
      
      // Stop all the current threads 
      sSocketName is string
      
      CriticalSectionStart("Management")
      
      EventChange("TERM",eventOpen)
      
      // Stop the different threads
      FOR ALL ROW of Table1
      	// Retrieve the connection settings
      	sSocketName = Socket
      	
      	// Closing the socket
      	IF SocketExist(sSocketName) = True THEN
      		SocketClose(sSocketName)
      	END
      END
      
      Multitask(-2)
      
      // Close the listening socket
      WHEN EXCEPTION IN
      	SocketClose(csNameListeningSocket)
      DO
      	EndProgram(True)
      END
      
      CriticalSectionEnd("Management")
