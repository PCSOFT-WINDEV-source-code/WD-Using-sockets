     code : |1-
      PROCEDURE UpdateTable(sSocketName,sIP)
      
      nSubscript is int
      
      
      CriticalSectionStart("Management")
      
      // Check whether this connection is already referenced in the table
      nSubscript = TableSeek(Table1.Socket,sSocketName)
      
      // New connection?
      IF nSubscript = -1 THEN
      	// New connection
      	TableAddLine(Table1,sIP,sSocketName,Now())
      ELSE
      	// Refresh the connection
      	TableModifyLine(Table1,nSubscript,sIP,sSocketName,Now())
      END
      
      
      CriticalSectionEnd("Management")
     type : 458752
   -
     name : ManageMessage
     internal_properties : CAAAAAgAAABkgyVDoxewVvmrY7OcAk/D2E4cDDFlaoPptFSb4/XwaJTOneeuFCFOknlCRUxkjIuhFm95pwLfJ4PVQYSwIYdRqzedpP1MiQ/eFfVc6HgKHz18IbDUUCahaRb7uR0e3zSD0+hkZm4AO+ynBMyfsowiKV+Bna24gJ6WR8xYoBjraA==
     procedure_id : 1760358308382188214
     type_code : 14
     code : |1-
      PROCEDURE ManageMessage(sMessage)
      
      CriticalSectionStart("Management")
      
      sSocketName is string
      
      // Send the message to all connected users
      FOR ALL ROW of Table1
      	
      	// Retrieve the name of the socket
      	sSocketName = Socket
      	
      	// Check the existence of this socket 
      	IF SocketExist(sSocketName) THEN		
      		// Send the message
      		SocketWrite(sSocketName,sMessage)	
      	END
      END
      
      CriticalSectionEnd("Management")
     type : 458752
