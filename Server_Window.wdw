      // Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be run in a secure thread (without calling ThreadExecute), without using HFSQL
      // 
      PROCEDURE ListenConnections()
      
      sChannelName is string
      sThreadName is string
      
      // Wait for connections
      LOOP	
      	ThreadPause(2)
      	
      	IF SocketWaitForConnection(csNameListeningSocket) THEN
      		
      		// New request for connection
      		// Accept the connections
      		sChannelName = SocketAccept(csNameListeningSocket)
      		
      		// Check the acceptance operation
      		IF sChannelName~="" THEN
      			ErrorAsynchronous(<ยง$0001ยง>,ErrorInfo())
      			BREAK
      		ELSE
      			// Start a thread to manage this connection
      			sThreadName = sChannelName
      			ManageConnection(sChannelName)
      			
      		END
      	END	
      END
     type : 458752
   -
     name : ManageConnection
     internal_properties : CAAAAAgAAADG0S4vfne17JjSuPqsBxsGIbBUlblts2BSPD042K3tqPQUY47NSMm69IFU2kbymTTuWtqvkb8oFNkt+6qLauwghy8fDM0ifNCCqg+Ncf5SZHcrqZXkH0WH40Gqh/bO7Ys4mPG3imUQI7jtygILQHuzDbDN/nHPgdqSE4MkGoUazDFklV3Z
     procedure_id : 1760358308382057142
     type_code : 14
     code : |1-
      // Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be run in a secure thread (without calling ThreadExecute), without using HFSQL
      // 
      PROCEDURE ManageConnection(sConnectionName)
      
      sMessage is string
      sIp is string = SocketClientInfo(sConnectionName,SocketAddress)
      
      // Read the messages
      LOOP
      	
      	IF EventWait("TERM",5) THEN BREAK
      	
      	UpdateTable(sConnectionName,sIp)
      	sMessage = SocketRead(sConnectionName,False,10)
      	
      	SWITCH sMessage
      		CASE "BYE" :
      			// User exit
      			BREAK
      			
      		CASE "" :
      			// Non-locking read operation, back to the loop
      			ThreadPause(1)
      			
      		OTHER CASE :
      			// Message to post			
      			ManageMessage(sMessage)			
      	END
      END
      
      // Closing the socket
      SocketClose(sConnectionName)
